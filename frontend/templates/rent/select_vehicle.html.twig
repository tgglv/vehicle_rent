<div id="select-vehicle-container">
    <form>
        <div class="form-group">
            <label for="type">Тип ТС</label>
            <select class="form-control" id="type">
                <option value="-1">Не указан</option>
            </select>
        </div>
        <div class="form-group">
            <label for="vendor">Марка</label>
            <select class="form-control" id="vendor">
                <option value="-1">Не указана</option>
            </select>
        </div>
        <button type="button" class="btn btn-primary" onclick="findVehicleToRent();">Найти ТС</button>
        <button type="button" class="btn btn-light" onclick="clearFilterVehicleSelect();">Очистить фильтр</button>
    </form>

    <table class="table" style="display: none;" id="tbl-vehicles-to-rent">
        <thead>
        <tr>
            <th>Тип</th>
            <th>Марка</th>
            <th>Модель</th>
            <th>Номер</th>
            <th></th>
        </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
</div>